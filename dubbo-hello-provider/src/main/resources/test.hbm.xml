<hibernate-mapping>
    <class name="beans.Customers" table="customers" >
        <id name="id" type="java.lang.Integer">
            <column name="ID" />
            <generator class="native" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="NAME" length="15" />
        </property>
    </class>
</hibernate-mapping>
